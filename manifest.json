{
  "name": "esa cautioner",
  "version": "1",
  "manifest_version": 2,
  "description": "This is a chrome extension to warn about articles that may be old in esa",
  "content_scripts": [{
    "matches": ["https://*.esa.io/posts/*"],
    "js": ["script.js"]
  }],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
